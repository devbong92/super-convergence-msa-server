{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "start:distributor": {
      "dependsOn": ["build"],
      "cache": false
    },
    "start:gate": {
      "dependsOn": ["start:distributor"],
      "cache": false
    },
    "start:ice": {
      "dependsOn": ["start:gate"],
      "cache": false
    },
    "start:auth": {
      "dependsOn": ["start:ice"],
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "auth": {
      "cache":false
    },
    "cache": {
      "cache":false
    }
  }
}
